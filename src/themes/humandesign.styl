@import "../../node_modules/jeet"

//debug($color: blue, $important: false)
clipnames = webkit clip

//===========================================
//     all colors with capital "C" prefix
//============================================
Cvertical  = #a5d2c1
Cabout_txt = #aeccd9
Cabout_bg  = #2f5a6b
Cabout_bg2 = #063f60
Clink      = #ffc905
ClinkHover = #ff0063




//========================================
//          general styles
//=======================================
body
   background: lighten(#ededed, 6%)
   font-family: 'Georgia', sans-serif
   text-align: center
   line-height: 1.5

h1
   font-weight: 700
h1 ~ p
   color: tomato
   font-style: italic
h2
   font-size 2rem
   font-weight bold
h2, h3, h4
   color #bdb46f
   font-family Roboto
   font-weight: normal
   margin-bottom 0
h3
   font-weight bold
h4
   getFonts()
   font-size 12pt
   margin-top 0
   margin-bottom 5px
pre
   margin: 0
   float : left
   width : 170px
   clear : both


hr
   border-left:0px
   border-top: 0px
   border-color: Cvertical
   color: Cvertical
a
   font-weight: bold
   color: Clink
a:hover
   font-weight: bold
   color: ClinkHover

.mail-text
   color: Cabout_txt
   font-weight: normal
   font-size: 10pt
   font-family: monospace


section
   p
      text-align: left
   input
      position: relative
      background: white !important
      text-align: center
   pre
      font-size: 10pt



KabalaWeight_p=20px
KabalaWeight_w=200px

.kabala_options_weight
      width: KabalaWeight_w
      position: absolute
      top: 50px
      right: KabalaWeight_p
      label
            min-width: 70px
      input
            width:70px
      ul
            text-align: right
            st-style-type: none
            list-style: none
            line-height: 0.2

.kabala_gates_count_for_channel
      width: 60px
      position: absolute
      top: 50px
      right: KabalaWeight_w + KabalaWeight_p
      pre
            text-align: left



@keyframes animKabalaLoading
   from
      opacity: 0
   to
      opacity: 1


@keyframes animKabalaLoaded
   from
      opacity: 1
   to
      opacity: 0


.inTextContent
   margin 0
   //padding-left 1rem
   text-align left

section#humandesignComp
   width:100%
   & #birthField
     z-index: 1
   & #kabalaLoading
      background #000000bb
      width 100%
      height 100vh
      position absolute
      display block
      text-align center
      vertical-align middle
      color: white
      opacity 1
      z-index 0
      padding-top 35vh
      word-wrap break-word
      white-space pre-wrap


   & .loading
      animation: animKabalaLoading 1s cubic-bezier(0.7,0,0.3,1) forwards
   & .loaded
      animation-delay 0.6s
      animation: animKabalaLoaded 1s cubic-bezier(0.7,0,0.3,1) forwards
   & .failed
      color: red !important
      cursor: pointer

   & .print_title
      background grey
      font-weight:bold
      font-size:1rem
      line-height 3.5rem
   & .print_content
      background: #bfb8a5
      width 100%
      font-weight 100
      margin 0

html
   background: #005556  !important /* Old browsers */
   background: -moz-linear-gradient(-45deg, #005556 0%, #ffb600 78%)  !important
   background: -webkit-linear-gradient(-45deg, #005556 0%,#ffb600 78%)  !important
   background: linear-gradient(135deg, #005556 0%,#ffb600 78%)  !important
   background-repeat: no-repeat !important
   background-attachment: fixed !important



