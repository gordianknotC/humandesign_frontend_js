      width:100%

.tyny_icon::before, .statistic_icon::before, .users_icon::befoore, .setting_icon::before, .list_icon::before, list_icon2::before, check_icon::before
   content ""
   display inline-block
   width 16px
   height 16px

.check_icon::before
   background-image: url('data:image/svg+xml;utf8;base64,')

.list_icon::before
   background-image: url('data:image/svg+xml;utf8;base64,')
.list_icon2::before
   background-image: url('data:image/svg+xml;utf8;base64,')
.statistic_icon::before
   background-image: url('data:image/svg+xml;utf8;base64,')

.setting_icon::before
   background-image: url('data:image/svg+xml;utf8;base64,')

.users_icon::before
   background-image: url('data:image/svg+xml;utf8;base64,')


div.dg
   & li
      & span.property-name
         width 30%
      & div.c
         width 70%
         & input[type=text]
            width 100%
   & li.has-slider
      & span.property-name
         width 30%
      & div.c
         width 70%
         & div.slider
            width 78%
         & input[type=text]
            width 20%

   & li.save-row
      background: #243e21
      & select
         background: #2e2e2e;
         border: none;
         color: #fff;
      & span
         background-color #1a80a3 !important
         border none !important
         text-shadow none !important
         box-shadow none !important

   & li.save-row::before, li.title::before
      content ""
      display inline-block
      width 16px
      height 16px
      vertical-align sub
      background-size contain